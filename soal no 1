#include <iostream>

using namespace std;

int main()
{
    int i,n,x,maksimal;
    cout <<"masukan nilai angka= ";
    cin>>n;
    maksimal=27;
    for
        (i=0;i<=n;i++)
    {
        cout<<"masukan angka"<<i<<" =";
        cin>>x;
        maksimal=maksimal+x;
    }
    cout<< "maksimal angka = "<<maksimal<<endl;
    return 0;
}
